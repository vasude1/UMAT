doi = {10.1016/S0020-7683(02)00651-0},
file = {:home/vasudevan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Anand, Gurtin - 2003 - A theory of amorphous solids undergoing large deformations, with application to polymeric glasses.pdf:pdf},
