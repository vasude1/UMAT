keywords = {Membranes,Nonlinear single integral constitutive equations,Uniaxial and tension-torsion histories,Volterra integral equations},
number = {3},
pages = {300--366},
title = {{Nonlinear viscoelastic solids - A review}},
volume = {14},
year = {2009}
