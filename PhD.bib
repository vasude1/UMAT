journal = {International Journal of Solids and Structures},
keywords = {Cohesive zone model,Thermomechanical coupling,Viscoelasticity},
number = {25-26},
pages = {4279--4291},
publisher = {Elsevier Ltd},
title = {{A thermomechanically coupled viscoelastic cohesive zone model at large deformation}},
url = {http://dx.doi.org/10.1016/j.ijsolstr.2013.08.031},
volume = {50},
year = {2013}
}
