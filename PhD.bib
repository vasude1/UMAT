file = {:home/vasudevan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yin, Kaliske - 2019 - Fracture simulation of viscoelastic polymers by the phase-field method.pdf:pdf},
issn = {14320924},
journal = {Computational Mechanics},
keywords = {Large deformation,Phase-field method,Rate-dependent fracture,Viscoelasticity},
publisher = {Springer Berlin Heidelberg},
title = {{Fracture simulation of viscoelastic polymers by the phase-field method}},
