isbn = {0021-8928},
issn = {00218928},
journal = {Journal of Applied Mathematics and Mechanics},
month = {jan},
number = {2},
pages = {469--477},
pmid = {1000000221},
publisher = {Pergamon},
title = {{On the nonsteady propagation of cracks}},
