file = {:home/vasudevan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mandal, Nguyen, Wu - 2019 - Length scale and mesh bias sensitivity of phase-field models for brittle and cohesive fracture.pdf:pdf;:home/vasudevan/Downloads/pfm-numerics.pdf:pdf},
